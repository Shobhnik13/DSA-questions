#include <iostream>
using namespace std;
char getmax(string s)
{
  int ar[26]={0};
  for(int i=0;i<s.length();i++)
    {
      int num=0;
      if(s[i]>='a' && s[i]<='z'){
num=s[i]-'a';
        }
      else{
num=s[i]-'A';
        }
      ar[num]++;
      }
int maxi=-1;
  int ans=0;
  for(int i=0;i<26;i++)
    {
if(maxi<ar[i]){
  maxi=ar[i];
      ans=i;
      }
      }
  return ans+'a';
  }
int main()
{
char name[100];
 cin>>name;
  cout<<getmax(name);
  return 0;
  }
